<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#66CCFF" >

    <Button
        android:id="@+id/personalAssistantButton"
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="90dp"
         android:textColor="@drawable/textdrawble"
android:background="@drawable/btndrawable"
        android:text="@string/personalassistant"
        
        android:textSize="25dp" />

    <Button
        android:id="@+id/healthCenterButton"
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="90dp"
        android:layout_toRightOf="@+id/personalAssistantButton"
         android:textColor="@drawable/textdrawble"
android:background="@drawable/btndrawable"
        android:text="@string/healthcenter"
        
        android:textSize="25dp" />

    <Button
        android:id="@+id/myPushButton"
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:layout_below="@+id/personalAssistantButton"
        android:layout_toLeftOf="@+id/healthCenterButton"
        android:layout_marginTop="5dp"
         android:textColor="@drawable/textdrawble"
android:background="@drawable/btndrawable"
        android:text="@string/mypush"
        
        android:textSize="25dp" />

    <Button
        android:id="@+id/myWantButton"
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/healthCenterButton"
        android:layout_alignTop="@+id/myPushButton"
        android:textColor="@drawable/textdrawble"
android:background="@drawable/btndrawable"
        android:text="@string/mywant"
        
        android:textSize="25dp" />

    <Button
        android:id="@+id/informationButton"
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/myPushButton"
        android:layout_below="@+id/myPushButton"
        android:layout_marginTop="5dp"
         android:textColor="@drawable/textdrawble"
android:background="@drawable/btndrawable"
        android:text="@string/information"
        
        android:textSize="25dp" />

    <Button
        android:id="@+id/publicSpaceButton"
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/myWantButton"
        android:layout_below="@+id/myWantButton"
        android:layout_marginTop="5dp"
         android:textColor="@drawable/textdrawble"
android:background="@drawable/btndrawable"
        android:text="@string/publicspace"
     
        android:textSize="25dp" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="90dp"
        android:layout_height="120dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:src="@drawable/elfin2" />

</RelativeLayout>